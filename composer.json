{
  "name": "contributte/menu-control",
  "type": "library",
  "description": "Menu control for Nette framework",
  "keywords": [
    "nette",
    "menu",
    "control",
    "component",
    "breadcrumbs"
  ],
  "homepage": "https://github.com/contributte/menu-control",
  "license": "MIT",
  "authors": [
    {
      "name": "David Kudera",
      "homepage": "https://kudera.dev"
    },
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Tomáš Jacík",
      "homepage": "https://tomasjacik.cz"
    }
  ],
  "support": {
    "issues": "https://github.com/contributte/menu-control/issues"
  },
  "require": {
    "php": ">=8.0 <8.3",
    "nette/di": "^3.0.9",
    "nette/utils": "^3.2.5",
    "nette/application": "^3.1.6",
    "nette/http": "^3.1.2"
  },
  "require-dev": {
    "nette/bootstrap": "^3.1.2",
    "latte/latte": "^3.0.3",
    "tracy/tracy": "^2.9",
    "mockery/mockery": "^1.5",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.13",
    "phpstan/phpstan": "^1.8.8",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.1",
    "phpstan/phpstan-strict-rules": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\MenuControl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures",
      "Tests\\Toolkit\\": "tests/toolkit"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
